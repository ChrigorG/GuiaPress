<%- include('../../partils/header')%>
<body>
    <%- include('../../partils/navbar')%>
    <div class="container mt-4"> 
        <a href="/admin/users/new" class="btn btn-success mb-4 mt-4">Cadastrar</a>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Login</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(users => {%>
                    <tr>
                        <th><%= users.id %></th>
                        <th><%= users.login %></th>
                    </tr>
                <% }) %>
            </tbody>
        </table>
        <br>
    </div>
    <br>
</body>
<%- include('../../partils/footer')%>